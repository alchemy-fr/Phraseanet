{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "alchemy-fr/tcpdf-clone": "~6.0",
        "alchemy/embed-bundle": "^0.3.4",
        "alchemy/geonames-api-consumer": "~0.1.0",
        "alchemy/oauth2php": "1.0.0",
        "alchemy/phlickr": "0.2.9",
        "alchemy/queue-bundle": "^0.1.5",
        "alchemy/rest-bundle": "^0.0.5",
        "alchemy/worker-bundle": "^0.1.6",
        "beberlei/assert": "^2.3",
        "cocur/slugify": "^2.0",
        "dailymotion/sdk": "~1.5",
        "dflydev/doctrine-orm-service-provider": "~1.0",
        "doctrine/cache": "1.8.0",
        "doctrine/dbal": "^2.4.0",
        "doctrine/orm": "^2.4.0",
        "elasticsearch/elasticsearch": "~2.0",
        "facebook/graph-sdk": "^5.6",
        "firebase/php-jwt": "^3.0.0",
        "gedmo/doctrine-extensions": "~2.3.0",
        "goodby/csv": "^1.3.0",
        "google/apiclient": "^2.0",
        "google/recaptcha": "^1.1",
        "igorw/get-in": "~1.0",
        "imagine/imagine": "0.6.x@dev",
        "ircmaxell/random-lib": "~1.0",
        "jms/serializer": "~0.10",
        "jms/translation-bundle": "^1.4",
        "justinrainbow/json-schema": "2.0.3",
        "league/flysystem": "^1.0",
        "league/flysystem-aws-s3-v2": "^1.0",
        "league/fractal": "0.13.0",
        "monolog/monolog": "~1.3",
        "mrclay/minify": "~2.1.6",
        "neutron/recaptcha": "~0.1.0",
        "neutron/temporary-filesystem": "~2.1",
        "pagerfanta/pagerfanta": "^1.0",
        "phpexiftool/exiftool": "10.10",
        "ramsey/uuid": "^3.0",
        "roave/security-advisories": "dev-master",
        "sensio/framework-extra-bundle": "^5.1",
        "simple-bus/doctrine-orm-bridge": "^4.0",
        "simple-bus/jms-serializer-bridge": "^1.0",
        "simple-bus/message-bus": "^2.1",
        "simple-bus/serialization": "^2.0",
        "symfony/asset": "*",
        "symfony/console": "*",
        "symfony/expression-language": "*",
        "symfony/flex": "^1.1",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/http-foundation": "*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "*",
        "symfony/security-bundle": "*",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "4.1.*",
        "symfony/twig-bundle": "*",
        "symfony/validator": "*",
        "symfony/web-link": "*",
        "symfony/yaml": "*",
        "themattharris/tmhoauth": "~0.7",
        "twig/extensions": "^1.5",
        "twig/twig": "^2.5",
        "vierbergenlars/php-semver": "~2.1",
        "webmozart/json": "^1.1",
        "willdurand/negotiation": "^2.0.0-alpha1",
        "zend/gdata": "~1.12.1"
    },
    "require-dev": {
        "symfony/debug-pack": "*",
        "symfony/dotenv": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "^1.0",
        "symfony/web-server-bundle": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "psr-0": {
            "Alchemy\\": "lib",
            "": "lib/classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        },
        "psr-0": {
            "Alchemy\\": "lib",
            "": "lib/classes"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.1.*",
            "public-dir": "public"
        }
    }
}
