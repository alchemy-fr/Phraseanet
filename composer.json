{
    "name"        : "phraseanet/phraseanet",
    "description" : "Phraseanet",
    "license"     : "GPL-3.0",
    "config": {
        "bin-dir" : "bin/"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/alchemy-fr/tcpdf-clone"
        },
        {
            "type": "vcs",
            "url": "https://github.com/alchemy-fr/imagine"
        },
        {
            "type": "git",
            "url": "https://github.com/bburnichon/Twig-extensions"
        },
        {
            "type": "git",
            "url": "https://github.com/alchemy-fr/symfony-cors"
        }
    ],
    "require": {
        "php": ">=5.4",
        "alchemy-fr/tcpdf-clone": "~6.0",
        "alchemy/geonames-api-consumer": "~0.1.0",
        "alchemy/google-plus-api-client": "~0.6.2",
        "alchemy/oauth2php": "1.0.0",
        "alchemy/phlickr": "0.2.9",
        "alchemy/phpexiftool": "^0.5.0",
        "alchemy/rest-bundle": "master@dev",
        "alchemy/symfony-cors": "^0.1.0",
        "beberlei/assert": "^2.3",
        "dailymotion/sdk": "~1.5",
        "data-uri/data-uri": "~0.1.0",
        "doctrine/orm": "~2.3.0",
        "facebook/php-sdk": "~3.0",
        "firebase/php-jwt": "^3.0",
        "gedmo/doctrine-extensions": "~2.3.0",
        "goodby/csv": "dev-master@dev",
        "guzzle/guzzle": "~3.0",
        "imagine/imagine": "dev-alchemy-0.6.2 as 0.6.2",
        "ircmaxell/random-lib": "^1.1",
        "jms/serializer": "~0.10",
        "justinrainbow/json-schema": "~1.3",
        "media-alchemyst/media-alchemyst": "^0.5",
        "mediavorus/mediavorus": "~0.4.0",
        "monolog/monolog": "~1.3",
        "mrclay/minify": "~2.1.6",
        "neutron/recaptcha": "~0.1.0",
        "neutron/silex-filesystem-provider": "~1.0",
        "neutron/silex-imagine-provider": "~0.1.0",
        "neutron/sphinxsearch-api": "~2.0.6",
        "neutron/temporary-filesystem": "~2.1",
        "pagerfanta/pagerfanta": "^1.0",
        "php-ffmpeg/php-ffmpeg": "~0.5.0",
        "php-xpdf/php-xpdf": "~0.2.1",
        "ramsey/uuid": "^2.8",
        "silex/silex": "^1.3.0",
        "silex/web-profiler": "~1.0.0",
        "simple-bus/doctrine-orm-bridge": "^4.0",
        "simple-bus/jms-serializer-bridge": "^1.0",
        "simple-bus/message-bus": "^2.1",
        "simple-bus/serialization": "^2.0",
        "swiftmailer/swiftmailer": "~4.3.0",
        "symfony/symfony": "~2.3.7",
        "themattharris/tmhoauth": "~0.7",
        "twig/extensions": "dev-improved-i18n",
        "twig/twig": "~1.14, >=1.14.2",
        "vierbergenlars/php-semver": "~2.1",
        "willdurand/negotiation": "^2.0.0-alpha1",
        "zend/gdata": "~1.12.1"
    },
    "require-dev": {
        "doctrine/data-fixtures"           : "~1.0.0",
        "phpunit/phpunit"                  : "~3.7",
        "behat/behat"                      : "~2.4",
        "behat/mink"                      : "~1.4",
        "behat/mink-extension"             : "~1.0",
        "behat/mink-goutte-driver"         : "~1.0",
        "behat/mink-selenium2-driver"      : "~1.0",
        "fabpot/goutte"                    : "~1.0"
    },
    "autoload": {
        "psr-0": {
            ""               : "lib/classes",
            "Alchemy"        : "lib",
            "Doctrine"       : "lib",
            "Entities"       : "lib/Doctrine",
            "Types"          : "lib/Doctrine",
            "Repositories"   : "lib/Doctrine",
            "Proxies"        : "lib/Doctrine",
            "PhraseaFixture" : "lib/conf.d"
        }
    },
    "include-path": ["vendor/zend/gdata/library"]
}
